{
  "name": "stellovault-server",
  "version": "1.0.0",
  "description": "TypeScript backend for StelloVault",
  "main": "dist/app.js",
  "type": "module",
  "scripts": {
    "start": "node dist/app.js",
    "dev": "ts-node-dev --respawn --transpile-only src/app.ts",
    "build": "tsc",
    "compile": "tsc --w",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev && prisma generate"
  },
  "dependencies": {
    "@prisma/adapter-pg": "^7.4.1",
    "@prisma/client": "^7.4.1",
    "@prisma/config": "^7.4.0",
    "@stellar/stellar-sdk": "^12.3.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "helmet": "^7.0.0",
    "jsonwebtoken": "^9.0.3",
    "morgan": "^1.10.0",
    "pg": "^8.18.0",
    "uuid": "^13.0.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/morgan": "^1.9.4",
    "@types/node": "^20.19.33",
    "@types/pg": "^8.16.0",
    "@types/uuid": "^10.0.0",
    "prisma": "^7.4.0",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.1.6"
  }
}
